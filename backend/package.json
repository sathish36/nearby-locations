{
  "name": "gc-ts-express",
  "version": "1.0.0",
  "description": "GC ts-express template",
  "main": "src/app.ts",
  "scripts": {
    "start": "node build/app.js",
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./build",
    "start:dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/app.ts",
    "start:debug": "node --inspect-brk=0.0.0.0 -r ts-node/register -r tsconfig-paths/register src/app.ts",
    "lint": "eslint 'src/**'",
    "lint:fix": "eslint --fix 'src/**'",
    "pretty": "prettier --write 'src/**'",
    "pretty:check": "prettylint",
    "test": "jest"
  },
  "author": "Sathish Gajarla <sathishgajarla@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "helmet": "^5.0.2",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "routing-controllers-openapi": "^3.1.0",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/body-parser": "^1.19.2",
    "@types/jest": "^27.4.1",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-typescript": "^16.1.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-sort-class-members": "^1.14.1",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "prettylint": "^1.0.0",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.13.0",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^4.6.2"
  },
  "keywords": []
}
